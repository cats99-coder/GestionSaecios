{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "saecios": {
      "type": "application",
      "root": "apps/saecios",
      "entryFile": "main",
      "sourceRoot": "apps/saecios/src",
      "compilerOptions": {
        "tsConfigPath": "apps/saecios/tsconfig.app.json"
      }
    },
    "productos": {
      "type": "application",
      "root": "apps/productos",
      "entryFile": "main",
      "sourceRoot": "apps/productos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/productos/tsconfig.app.json"
      }
    },
    "pagos": {
      "type": "application",
      "root": "apps/pagos",
      "entryFile": "main",
      "sourceRoot": "apps/pagos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/pagos/tsconfig.app.json"
      }
    },
    "gastos": {
      "type": "application",
      "root": "apps/gastos",
      "entryFile": "main",
      "sourceRoot": "apps/gastos/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gastos/tsconfig.app.json"
      }
    },
    "metodos-pago": {
      "type": "application",
      "root": "apps/metodos-pago",
      "entryFile": "main",
      "sourceRoot": "apps/metodos-pago/src",
      "compilerOptions": {
        "tsConfigPath": "apps/metodos-pago/tsconfig.app.json"
      }
    },
    "reportes": {
      "type": "application",
      "root": "apps/reportes",
      "entryFile": "main",
      "sourceRoot": "apps/reportes/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reportes/tsconfig.app.json"
      }
    }
  }
}